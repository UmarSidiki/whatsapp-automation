# Copy this file to .env and adjust values before running in production
PORT=3000
LOG_LEVEL=info
NODE_ENV=production
AUTH_CODES=replace-with-code-1,replace-with-code-2
JSON_BODY_LIMIT=1mb
RATE_LIMIT_MAX=120
AUTH_RATE_LIMIT_MAX=20
AI_TIMEOUT_MS=30000  # Increased to 30s to accommodate voice message processing (transcription + AI + TTS)
PUPPETEER_HEADLESS=true
GEMINI_BASE_URL=https://generativelanguage.googleapis.com
# STATIC_DIR=absolute\path\to\custom\frontend
MONGO_URI=mongodb://localhost:27017
MONGO_DB_NAME=whatsapp_automation
ENABLE_COMPRESSION=true
ENABLE_REQUEST_LOGGER=false
AUTO_RESTORE_SESSIONS=true
SESSION_RESTORE_THROTTLE_MS=1000
